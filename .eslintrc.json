{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [ "google"],
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "max-len": [
            "error",
            120
        ],
        "no-console": 0
    }
}
